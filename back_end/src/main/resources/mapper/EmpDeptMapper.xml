<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.model.mapper.EmpDeptMapper">
    <insert id="addEmpDept">
        INSERT INTO emp_dept(emp_id,dept_id) VALUES (#{empId},#{deptId})
    </insert>

    <delete id="delByEmpId">
        DELETE FROM emp_dept WHERE emp_id = #{empId}
    </delete>

    <delete id="delByDeptId">
        DELETE FROM emp_dept WHERE dept_id = #{deptId}
    </delete>
</mapper>